{
  "name": "gatsby-source-strava",
  "version": "2.0.0-beta.11",
  "description": "Gatsby plugin to use Strava as a data source",
  "repository": "https://github.com/xuopled/gatsby-source-strava",
  "author": "CÃ©dric Delpoux",
  "license": "MIT",
  "private": false,
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source",
    "strava"
  ],
  "main": "index.js",
  "dependencies": {
    "glob": "^7.1.6",
    "inquirer": "^7.0.6",
    "strava-v3": "^2.0.5",
    "system-sleep": "^1.3.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "1.19.1"
  },
  "peerDependencies": {
    "gatsby": "^2.0.0"
  },
  "scripts": {
    "build": "cross-env BABEL_ENV=production babel src --out-dir .",
    "lint": "eslint --ext .js src",
    "prepublishOnly": "yarn build",
    "token": "./scripts/generate-token.js "
  },
  "bin": {
    "gatsby-source-strava-token": "./scripts/generate-token.js"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ],
    "*.{css,js,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
